@using TravelTripProject.Models.Classes
@model List<Blog>
<div class="padding1">
    <h4>En Beğenilen Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src="@x.BlogImage" alt="" />
            </div>
            <div class="bot-desc">
                <p>@x.Description.Substring(0, x.Description.Substring(0, 80).LastIndexOf(" ")) ...</p>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5", "Default")
</div>

